{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "PVZ_Hybrid",
  "version": "0.1.1",
  "identifier": "top.goteam",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "植物大战僵尸杂交版 v3.9",
        "width": 500,
        "height": 350
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
