{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "star",
  "identifier": "top.goteam",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "star",
        "width": 1000,
        "height": 700,
        "center": true,
        "transparent": true,
        "windowEffects": {
          "effects": [
            "hudWindow",
            "acrylic",
            "mica"
          ],
          "radius": 12
        }
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/32x32.ico"
    ],
    "macOS": {
      "minimumSystemVersion": "12.0",
      "dmg": {
        "appPosition": {
          "x": 180,
          "y": 170
        },
        "applicationFolderPosition": {
          "x": 480,
          "y": 170
        },
        "windowSize": {
          "height": 400,
          "width": 660
        }
      }
    },
    "windows": {
      "nsis": null
    }
  }
}